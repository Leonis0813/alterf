<tr class='<%= analysis[:state] == 'complete' ? 'success' : 'warning' %>'>
  <td><%= analysis.created_at.strftime('%Y/%m/%d %H:%M:%S') %></td>
  <td><%= analysis[:num_data] %></td>
  <td><%= analysis[:num_tree] %></td>
  <td><%= analysis[:num_feature] %></td>
  <td><%= analysis[:state] == 'complete' ? '完了' : '実行中' %></td>
</tr>
